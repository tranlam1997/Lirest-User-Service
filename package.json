{
  "name": "lirest-user-service",
  "version": "0.1.0",
  "description": "User service for lirest project",
  "private": true,
  "main": "src/main.ts",
  "engines": {
    "node": ">= 16.x",
    "pnpm": ">= 7.x"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -b ./tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "start": "ts-node -r tsconfig-paths/register .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@tranlam1997/lirest-internal-communication-service": "^0.2.10",
    "bcryptjs": "^2.4.3",
    "cls-hooked": "^4.2.2",
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "cross-var": "^1.1.0",
    "express": "^4.18.2",
    "express-basic-auth": "^1.2.1",
    "express-rate-limit": "^6.5.1",
    "express-validator": "^6.14.0",
    "express-winston": "^4.2.0",
    "inversify": "^6.0.1",
    "joi": "^17.6.0",
    "lightship": "^7.1.1",
    "ms": "^2.1.3",
    "nodemon": "^2.0.19",
    "openapi-types": "^11.0.1",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "rimraf": "^3.0.2",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.4.0",
    "tsconfig-paths": "^3.14.1",
    "tsoa": "^5.0.0",
    "typeorm": "^0.3.6",
    "typeorm-naming-strategies": "^4.1.0",
    "winston": "^3.7.2",
    "winston-daily-rotate-file": "^4.7.1",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cls-hooked": "^4.3.3",
    "@types/config": "^0.0.41",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.0.0",
    "@types/ms": "^0.7.31",
    "@types/node": "^17.0.30",
    "@types/pg": "^8.6.6",
    "@types/request-ip": "^0.0.37",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "jest": "^29.0.2",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "tsc-alias": "^1.7.0",
    "typescript": "^4.6.3"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "src/**/*.spec.ts"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "collectCoverageFrom": [
      "src/**/*.ts",
      "test/**/*.ts",
      "!**/node_modules/**"
    ],
    "testEnvironment": "node",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover"
    ],
    "verbose": true,
    "moduleNameMapper": {
      "^@src/(.*)$": "<rootDir>/src/$1",
      "^@test/(.*)$": "<rootDir>/test/$1"
    }
  }
}
